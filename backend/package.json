{
  "name": "backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start:dev": "sls offline start",
    "deploy:dev": "sls deploy --verbose",

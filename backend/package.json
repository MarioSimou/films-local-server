    "@middy/http-cors": "^2.5.7",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/validator": "^2.5.7",
    "aws-multipart-parser": "^0.2.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.4",
    "esbuild": "^0.14.11",
